<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane onKeyPressed="#handle" prefHeight="400.0" prefWidth="600.0" styleClass="bodybg" stylesheets="@background.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.SettingsController">
   <children>
      <HBox prefHeight="28.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Button fx:id="fullScreenButton" mnemonicParsing="false" onAction="#fullScreenButtonClicked" prefHeight="25.0" prefWidth="75.0" text="Full Screen" />
         </children>
      </HBox>
   </children>
</AnchorPane>
