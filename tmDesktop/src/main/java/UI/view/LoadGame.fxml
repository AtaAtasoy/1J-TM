<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane onKeyPressed="#handle" prefHeight="400.0" prefWidth="600.0" styleClass="bodybg" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.LoadGameController">
   <children>
      <HBox prefHeight="26.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="25.0" prefWidth="63.0" text="Back" />
            <Button fx:id="fullScreenButton" mnemonicParsing="false" onAction="#fullScreenButtonClicked" text="Full Screen" />
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@background.css" />
      <URL value="@fontStyle.css" />
   </stylesheets>
</AnchorPane>
